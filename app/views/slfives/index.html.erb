<% title "SL5" %>

<table class="index-table">
  	<tr>
	    <th>Landing Center</th>
	    <th>Date</th>
	    <th>Gear</th>
	    <th>Serialno</th>
	    <th>Sampled</th>
	    <th>Wt Dealer</th>
	    <th>Wt Consumed</th>
	    <th>Wt Crew</th>
	    <th>Wt Others</th>
	</tr>
	<% for slfive in @slfives %>
	<tr>
	  	<td><%= slfive.landing_center %></td>
      	<td><%= slfive.date %></td>
      	<td><%= slfive.gear %></td>
      	<td><%= slfive.serialno %></td>
      	<td><%= slfive.sampled %></td>
      	<td><%= slfive.wt_dealer %></td>
      	<td><%= slfive.wt_consumed %></td>
      	<td><%= slfive.wt_crew %></td>
      	<td><%= slfive.wt_others %></td>
		<td>
			<%= link_to view_ico, slfive %>
      		<%= link_to edit_ico, edit_slfive_path(slfive) %>
      		<%= link_to delete_ico, slfive, :confirm => 'Are you sure?', :method => :delete %>
		</td>
	</tr>
	<% end %>
</table>

<p><%= link_to new_ico, new_slfife_path %></p>
