<% title "SL4" %>

<table class="index-table">
  	<tr>
		<th>Date</th>
		<th>Landing Center</th>
		<th>Gear</th>
		<th>Sampling Interval</th>
		<th>Vessel Serial No</th>
		<th>Vessel Name</th>
		<th>Vessel Size</th>
		<th>Sampled</th>
		<th></th>
	</tr>
  	<% for slfour in @slfours %>
    <tr>
      	<td><%= slfour.date %></td>
	    <td><%= slfour.LandingCenter %></td>
	    <td><%= slfour.Gear_id %></td>
	    <td><%= slfour.interval %></td>
	    <td><%= slfour.SerialNo %></td>
	    <td><%= slfour.BoatName %></td>
	    <td><%= slfour.BoatSize %></td>
	    <td><%= slfour.sampled %></td>
	    <td>	
			<%= link_to view_ico, slfour %>
	    	<%= link_to edit_ico, edit_slfour_path(slfour) %>
	      	<%= link_to delete_ico, slfour, :confirm => 'Are you sure?', :method => :delete %>
		</td>
    </tr>
  	<% end %>
</table>

<p><%= link_to new_ico, new_slfour_path %></p>
